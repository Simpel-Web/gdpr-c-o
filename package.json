{
    "name": "gdpr-c-o",
    "version": "0.0.2",
    "description": "GDPR compliant overlay tool where you can prevent unwanted loading of iFrames with videos or google maps.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint src/**/*.{ts} --quiet",
        "prepare": "husky install && npm run build",
        "release": "npm run lint -- --fix && standard-version",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "pre-commit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Simpel-Web/gdpr-c-o.git"
    },
    "keywords": [
        "GDPR",
        "click",
        "before",
        "consent"
    ],
    "author": "Stefan Herpich & Robert Krause",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.2.2",
        "@eslint/js": "^9.2.0",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.2",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "globals": "^15.1.0",
        "husky": "8.0.3",
        "lint-staged": "13.3.0",
        "pinst": "3.0.0",
        "standard-version": "9.5.0",
        "typescript": "5.3.3",
        "typescript-eslint": "^7.8.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,json}": [
            "eslint --fix"
        ]
    },
    "files": [
        "lib/**/*"
    ]
}
