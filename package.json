{
    "name": "gdpr-c-o",
    "version": "0.0.2",
    "description": "GDPR compliant overlay tool where you can prevent unwanted loading of iFrames with videos or google maps.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint src/**/*.{js,ts,tsx} --quiet",
        "prepare": "husky install && npm run build",
        "release": "npm run lint -- --fix && standard-version",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "pre-commit": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Simpel-Web/gdpr-c-o.git"
    },
    "keywords": [
        "GDPR",
        "click",
        "before",
        "consent"
    ],
    "author": "Stefan Herpich & Robert Krause",
    "license": "ISC",
    "devDependencies": {
        "@commitlint/cli": "19.3.0",
        "@commitlint/config-conventional": "19.2.2",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "9.2.0",
        "eslint-config-prettier": "8.10.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "lint-staged": "13.3.0",
        "pinst": "3.0.0",
        "prettier": "3.2.5",
        "standard-version": "9.5.0",
        "typescript": "5.4.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,json}": [
            "eslint --fix"
        ]
    },
    "files": [
        "lib/**/*"
    ]
}
